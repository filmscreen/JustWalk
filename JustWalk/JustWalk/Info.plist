<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>NSHealthShareUsageDescription</key>
	<string>JustWalk reads your step count and walking distance to track your daily progress.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>JustWalk saves your walking workouts to Apple Health.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Just Walk uses your location to track your walk route, even when the screen is locked.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Just Walk uses your location to track your walk route on the map.</string>
	<key>NSMotionUsageDescription</key>
	<string>Just Walk uses motion sensors to count your steps in real-time during walks.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Just Walk saves walk summary images to your photo library when you choose to save them.</string>
	<key>NSSupportsLiveActivities</key>
	<true/>
	<key>GEMINI_API_KEY</key>
	<string>$(GEMINI_API_KEY)</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>remote-notification</string>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.justwalk.widget-refresh</string>
		<string>com.justwalk.healthkit-sync</string>
	</array>
</dict>
</plist>
